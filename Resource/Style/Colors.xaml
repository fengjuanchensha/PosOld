<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:options="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <!--浅蓝色前景色-->
    <Color x:Key="LightBlueForground.Color">#FF0E6EDB</Color>
    <!--浅蓝色前景色-->
    <SolidColorBrush x:Key="LightBlueForground" Color="{StaticResource LightBlueForground.Color}"/>
    <!--深红前景色-->
    <SolidColorBrush x:Key="DarkRedForground" Color="#FFFE2036"/>
    <SolidColorBrush x:Key="LightBlueBackground" Color="#FF119EDA" options:Freeze="True" />
    <!--按钮正常边框色-->
    <SolidColorBrush x:Key="Button.BorderBrush.Normal" Color="#FF114D6D"/>

    <!--DataGrid 边框色-->
    <SolidColorBrush x:Key="DataGrid.Print.BorderThickness" Color="#FFA6A6A6"/>
    <!--按钮正常背景色-->
    <SolidColorBrush x:Key="Button.Background.Normal" Color="#FF119EDA" options:Freeze="True" />
    <SolidColorBrush x:Key="Button.Foreground.Normal" Color="White" options:Freeze="True" />
    <SolidColorBrush x:Key="Button.Background.Mousover" Color="#FF086F9E" options:Freeze="True" />
    <SolidColorBrush x:Key="Button.Foreground.Mousover" Color="White" options:Freeze="True" />
    <SolidColorBrush x:Key="Button.Background.Pressed" Color="#99119EDA" options:Freeze="True" />
    <SolidColorBrush x:Key="Button.Foreground.Pressed" Color="White" options:Freeze="True" />
</ResourceDictionary>